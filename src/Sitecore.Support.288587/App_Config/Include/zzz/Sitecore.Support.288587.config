      <processMessage>
        <processor type="Sitecore.Forms.Core.Pipelines.ProcessMessage, Sitecore.Forms.Core" method="SendEmail">
          <patch:attribute name="type">Sitecore.Support.Forms.Core.Pipelines.ProcessMessage, Sitecore.Support.288587</patch:attribute>
        </processor>
      </processMessage>
    </pipelines>
  </sitecore>
</configuration>